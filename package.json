{
  "name": "browsalyzer",
  "version": "0.14.0",
  "description": "Benchmark runner for trace metrics",
  "keywords": [
    "chrome",
    "metrics",
    "tracing"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/devtrace/browsalyzer.git"
  },
  "license": "BSD-2-Clause",
  "author": "Kris Selden <kris.selden@gmail.com>",
  "files": [
    "dist/index.d.ts",
    "dist/index.js",
    "dist/index.js.map",
    "dist/lib",
    "bin"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "plot": "./bin/plot.R",
    "report": "./bin/report.R",
    "runtime-stats": "./bin/runtime-stats.R"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "lint": "tslint -p tsconfig.json",
    "prepare": "tsc",
    "test": "tslint -p tsconfig.json && tsc && node dist/test/build.js && node dist/test.js",
    "test:build": "node dist/test/build.js",
    "test:only": "node dist/test.js"
  },
  "dependencies": {
    "@types/node": "*",
    "array-binsearch": "^1.0.1",
    "chrome-debugging-client": "^0.6.3"
  },
  "devDependencies": {
    "@types/uglify-js": "^2.6.31",
    "bower": "^1.7.9",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1",
    "uglify-js": "^2.8.29"
  },
  "engine": "node >= 8"
}
